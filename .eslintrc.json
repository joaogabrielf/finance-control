{
  "extends": [
    "@rocketseat/eslint-config/react",
    "plugin:tailwindcss/recommended"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js"
      ],
      "parser": "@typescript-eslint/parser",
      "plugins": [
        "@typescript-eslint"
      ],
      "rules": {
        "no-unused-vars": "warn"
      }
    }
  ]
}